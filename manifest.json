{
    "manifest_version": 3,
    "name": "Jimmy",
    "description": "Radical!",
    "version": "1.0",
    "icons": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    },
    "action": {
        "default_popup": "popup.html"
      },
      "permissions": ["activeTab", "scripting", "offscreen"],
      "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Alt+J",
            "mac": "Alt+B"
          }
        }
      }
  }